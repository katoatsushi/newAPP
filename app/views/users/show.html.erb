<div class="contents row">

  <p><%= current_user.clubname %>さんの投稿一覧</p>
		<% @my_events.each do |event| %>
		<!-- 自分の投稿したイベント一覧 -->
		  <div class= "box18">
		    <p>イベント名：<%= event.eventname %></p>
		    <p>参加費：<%= event.cost %></p>
		   </div>
        <% end %>
		<p>=============================================================</p>
		<!-- 以下、他の人のホームページに行った時 -->
	<% end %>
		<% @events.each do |event| %>
		<!-- 他の人のホームページの投稿一覧 -->
		  <div class= "box18">
		    <p>イベント名：<%= event.eventname %></p>
		    <p>参加費：<%= event.cost %></p>
		   </div>
		<% end %>
	<% end %>

</div>


